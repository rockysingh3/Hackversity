  <script type="text/javascript">
        $(document).ready(function(){
          $("#filter a").click(function(){
            $("#filter .current").removeClass("current");
            $(this).parent().addClass("current");
            var filterVal = $(this).data('index');
            $("#forms li").fadeIn();
            $("#forms li:not(."+ filterVal+")").hide();
      });
    });
  </script>

